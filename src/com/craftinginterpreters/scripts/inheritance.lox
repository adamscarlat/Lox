class Doughnut {
  cook() {
    print "Fry until golden brown.";
  }
}

class Organic {
    makeMoreExpensive() {
        print "Made more expensive";
    }
}

class Decorator {
    decorate() {
        print "Sprinkle sprinkle";
    }
}

class BostonCream < Doughnut, Organic , Decorator {
    cook() {
        print "Add cream";
        super.cook();
        super.makeMoreExpensive();
        super.decorate();
    }
}

BostonCream().cook();